citHeader("To cite netrankr in publications use:")

citEntry(entry = "Manual",
  title        = "netrankr:",
  author       = personList(as.person("David Schoch")),
  journal      = "",
  year         = "2017",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("David Schoch (2017).",
        "netrankr: R package for Partial Centrality and Expected Ranks in networks")
)